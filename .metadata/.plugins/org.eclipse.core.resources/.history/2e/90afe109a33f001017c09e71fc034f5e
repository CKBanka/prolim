package oops;

public class Car {
	int wheel;
	String colour;
	double price;
	void start() {
		System.out.println("Car started");
	}
	void appliedGear() {
		System.out.println("gear applied");
		
	}
	void moving() {
		System.out.println("Car is mvoing");
	}
	void stop() {
		System.out.println("Car stopped");
	}
	
}
