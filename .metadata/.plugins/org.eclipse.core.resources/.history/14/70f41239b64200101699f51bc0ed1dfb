package multithreading;

class A extends Thread{
	public void run() {
		for(int i=1;i<10;i++) {
			System.out.println(i);
		}
	}
}
class B extends Thread{
	public void run() {
		for(int i=1;i<10;i++) {
			System.out.println(i);
		}
	}
}
public class ExtendExam {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		A obj1= new A();
		B obj2= new B();
		obj1.start();
		obj2.start();
		
	}

}
